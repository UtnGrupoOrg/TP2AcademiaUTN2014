<configuration>
    <system.web>
      <pages validateRequest="true" theme="default"/>
      <compilation debug="true" targetFramework="4.0" />
      <authentication mode="Forms">
        <forms loginUrl="login.aspx" name=".ASPXFORMSAUTH">
        </forms>
      </authentication>
      <authorization>
        <deny users="?" />
      </authorization>
    </system.web>
   <connectionStrings>
      <add name="ConnStringLocal"
      providerName="System.Data.SqlClient"
      connectionString="Server=LOCALHOST;Database=Academia;Integrated Security=false; User=net; Password=net;Connection Timeout=5"/>
     <add name="ConnStringLocalPedro"
     providerName="System.Data.SqlClient"
     connectionString="Server=LOCALHOST;Database=Academia;Integrated Security=true;Connection Timeout=5"/>
      <add name="ConnStringExpress"
      providerName="System.Data.SqlClient"
      connectionString="Server=localhost\SQLEXPRESS;Database=Academia; Integrated Security=false; User=net; Password=net;Connection Timeout=5"/>
      <add name="ConnStringServerISI"
      providerName="System.Data.SqlClient"
      connectionString="Server=serverisi; Database=Academia; Integrated Security=false; User=net; Password=net;Connection Timeout=5"/>
   </connectionStrings>
</configuration>
